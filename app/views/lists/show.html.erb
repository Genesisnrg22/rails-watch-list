<!-- app/views/list/show.html.erb -->
<h2><%= @list.name %></h2>
<% @list.movies.each do |movie| %>
  <%= image_tag movie.poster_url, width: "100px" %>
  <%= movie.title %>
  <div>
    <% movie.bookmarks.each do |element| %>
      <%= element.comment %>
    <% end %>
  </div>
<% end %>
<%= link_to "< back", lists_path%>
<%= link_to " Add review", new_list_bookmark_path(@list)%>
